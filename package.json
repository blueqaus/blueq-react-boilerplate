{
  "name": "blueq-react-boilerplate",
  "version": "0.0.0",
  "description": "BlueQ's React App boilerplate",
  "main": "index.jsx",
  "scripts": {
    "start": "webpack-dev-server",
    "production": "webpack -p",
    "lint:es": "eslint app/. --ext=js --ext=jsx; exit 0",
    "lint:es:fix": "eslint app/. --ext=js --ext=jsx --fix; exit 0",
    "lint:sass": "sass-lint 'app/**/*.scss' --no-exit --verbose",
    "lint": "npm run lint:es; npm run lint:sass",
    "lint:fix": "npm run lint:es:fix; npm run lint:sass"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blueqaus/blueq-react-boilerplate.git"
  },
  "keywords": [
    "react",
    "react-router",
    "redux",
    "webpack2",
    "eslint",
    "react-boilerplate",
    "boilerplate",
    "blueq"
  ],
  "author": "Rhys Gevaux (https://twitter.com/vorksau)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blueqaus/blueq-react-boilerplate/issues"
  },
  "homepage": "https://github.com/blueqaus/blueq-react-boilerplate#readme",
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2",
    "sanitize.css": "^4.1.0",
    "webpack": "^2.2.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.6",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-preset-stage-1": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-preset-stage-3": "^6.22.0",
    "css-loader": "^0.26.2",
    "eslint": "^3.16.1",
    "eslint-config-blueq": "^1.0.9",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-babel": "^4.1.0",
    "eslint-plugin-immutablejs": "^0.1.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.3.3",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-lint": "^1.10.2",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}
